let fibonacci = recursive (
    \self -> \n ->
        if :< n 2
        then n
        else :+ (self (:- n 2)) (self (:- n 1))
)

let factorial = recursive (
    \self -> \n ->
        if := n 0
        then 1
        else :* n (self (:- n 1))
)

let xs = head (filter (\x -> :> x 5) (rangeInclusive 1 10))
